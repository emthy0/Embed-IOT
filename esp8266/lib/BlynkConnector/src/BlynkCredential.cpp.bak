#include <BlynkCredential.h>

BlynkCredential::BlynkCredential(){}

BlynkCredential::BlynkCredential(char* blynkTemplateId, char* blynkTemplateName, char* blynkAuthToken, char* blynkSsid, char* blynkPass)
{
    _BLYNK_TEMPLATE_ID = blynkTemplateId;
    _BLYNK_TEMPLATE_NAME = blynkTemplateName;
    _BLYNK_AUTH_TOKEN = blynkAuthToken;
    _BLYNK_SSID = blynkSsid;
    _BLYNK_PASS = blynkPass;
}

char* BlynkCredential::getBlynkTemplateId()
{
    return _BLYNK_TEMPLATE_ID;
}

char* BlynkCredential::getBlynkTemplateName()
{
    return _BLYNK_TEMPLATE_NAME;
}

char* BlynkCredential::getBlynkAuthToken()
{
    return _BLYNK_AUTH_TOKEN;
}

char* BlynkCredential::getBlynkSsid()
{
    return _BLYNK_SSID;
}

char* BlynkCredential::getBlynkPass()
{
    return _BLYNK_PASS;
}